export     CHOST="x86_64-apathy-linux-musl"
export    CBUILD="${CHOST}"
export MAKEFLAGS="-j2"

export        CC="${CHOST}-gcc"
export       CXX="${CHOST}-g++"

export    CFLAGS="-w -march=native -mtune=native -O3 -fstack-protector-strong"
export   LDFLAGS="${CFLAGS} -Wl,-O1 -Wl,--as-needed -Wl,--gc-sections"
export  CXXFLAGS="${CFLAGS}"
