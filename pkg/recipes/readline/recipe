# 1
pdir="/mss/repo/pkg/recipes/readline/patches"
patch -p1 < "${pdir}"/fix-ncurses-underlinking.patch

./configure \
 --build=$CBUILD           \
 --host=$CHOST             \
 --prefix=/usr             \
 --mandir=/usr/share/man   \
 --infodir=/usr/share/info \
 --disable-static          \
 --enable-shared
